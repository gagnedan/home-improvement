<!-- Begin block container -->
<div class="block block-bordered block-rounded">
  <!-- Begin block header -->
  <div class="block-header">
    <h2 class="block-title">Project</h2>
  </div>
  <!-- End block header -->
  <!-- Begin block content -->
  <div class="block-content">
    <%= form_with(model: project, url: admin_project_path, local: true) do |form| %>
      <%= errors_for(project) %>

      <div class="form-group">
        <%= form.label :name %>
        <%= form.text_field :name, id: :project_name, class:"form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :description %>
        <%= form.text_area :description, id: :project_description, class:"form-control", rows: 5%>
      </div>

      <div class="push-10-t text-center">
        <%= form.submit class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
  <!-- End block content -->
</div>
<!-- End block container -->
